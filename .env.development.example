# Development Environment Variables
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Auth Service Configuration
AUTH_SERVICE_URL=http://localhost:4000

# Pokemon API Service (your custom API)
POKEMON_API_BASE_URL=http://localhost:20275/api/v1

# CORS Settings
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Static Auth (for testing without auth service)
STATIC_AUTH_USER=test
STATIC_AUTH_PASS=test

# Logging
LOG_LEVEL=debug

# Health Check
HEALTH_CHECK_TIMEOUT=5000
